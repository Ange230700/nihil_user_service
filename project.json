{
  "name": "@nihil_backend/user",
  "root": "user",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run build",
        "cwd": "user"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run test",
        "cwd": "user"
      }
    },
    "test:ci": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run test:ci",
        "cwd": "user"
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run clean",
        "cwd": "user"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": { "command": "npm run lint", "cwd": "user" }
    },
    "prisma:generate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run prisma:generate",
        "cwd": "user"
      }
    },
    "prisma:migrate:deploy": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run prisma:migrate:deploy",
        "cwd": "user"
      }
    },
    "prisma:db:push:force": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run prisma:db:push:force",
        "cwd": "user"
      }
    }
  }
}
